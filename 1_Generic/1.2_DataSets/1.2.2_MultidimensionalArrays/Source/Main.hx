package;

class Main {
	public static function main () {
		//----------------------------------------------------------------------
		trace('--- TWO-DIMENSIONAL ARRAY WITH TWO ROWS ---');
		
		var a : Array<Array<Int>> = [ [], [] ]; // x, y
		a[0][0] = 0;
		a[0][1] = 1;
		a[0][2] = 2;
		a[1][0] = 10;
		a[1][1] = 11;
		a[1][2] = 12;
		trace(a[0]);
		trace(a[1]);
		
		//----------------------------------------------------------------------
		trace('--- THREE-DIMENSIONAL ARRAY WITH TWO ROWS ---');
		
		var b : Array<Array<Array<Int>>> = [ [ [], [] ], [ [], [] ] ]; // x, y, z
		b[0][0][0] = 0;
		b[0][0][1] = 1;
		b[0][0][2] = 2;
		b[0][1][0] = 10;
		b[0][1][1] = 11;
		b[0][1][2] = 12;
		b[1][0][0] = 100;
		b[1][0][1] = 101;
		b[1][0][2] = 102;
		b[1][1][0] = 1010;
		b[1][1][1] = 1011;
		b[1][1][2] = 1012;
		trace(b[0][0]);
		trace(b[0][1]);
		trace(b[1][0]);
		trace(b[1][1]);
	}
}
